# Required
GEMINI_API_KEY=your-gemini-key
KLING_ACCESS_KEY=your-access-key-id
KLING_SECRET_KEY=your-secret-key

# Optional
CREATIVE_VARIANTS_LOG_LEVEL=INFO

# Kling extend mode controls (optional)
# KLING_EXTEND_ENABLED=1               # Turn on multi-step extend flow
# KLING_EXTEND_PATH=/v1/videos/video-extend  # Override when using an aggregator proxy
# KLING_MODEL_NAME=kling-v1-6          # Model override if provider changes names
# KLING_ASPECT=9:16                    # Aspect ratio sent to Kling
# KLING_MODE=pro                       # Quality tier for generation
# KLING_MAX_DURATION=10                # Hard limit for the base clip in seconds
# KLING_EXTEND_STEP=5                  # Max seconds requested per extend call
# KLING_EXTEND_USE_URL=1               # Send init_video_url instead of task_id
# KLING_EXTERNAL_TASK_ID_PREFIX=prod   # Deterministic external_task_id prefix
